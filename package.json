{
  "name": "fan-electron",
  "description": "fan-electron 泛应用",
  "keywords": [
    "fan-electron",
    "泛应用",
    "electron",
    "React",
    "vite"
  ],
  "private": true,
  "version": "0.0.10",
  "author": {
    "name": "fxss5201",
    "email": "723107234@qq.com",
    "url": "https://github.com/fxss5201"
  },
  "homepage": "https://github.com/fxss5201/fan-electron",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "commit": "git-cz",
    "build-icon": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "build-mac": "tsc && vite build && electron-builder build --mac",
    "build-win": "tsc && vite build && electron-builder build --win",
    "build-linux": "tsc && vite build && electron-builder build --linux"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.6",
    "@ant-design/pro-components": "^2.7.0",
    "@reduxjs/toolkit": "^2.2.2",
    "ahooks": "^3.7.11",
    "antd": "^5.15.4",
    "dayjs": "^1.11.10",
    "electron-updater": "^6.1.8",
    "react": "^18.2.0",
    "react-activation": "^0.12.4",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.5",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "cz-git": "^1.9.1",
    "electron": "^29.1.4",
    "electron-builder": "^24.13.3",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.js",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
